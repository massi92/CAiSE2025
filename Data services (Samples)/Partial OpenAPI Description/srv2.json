{
  "openapi": "3.1.0",
  "info": {
    "title": "srv2",
    "version": "1.0.0",
    "x-service-type": "COLLECT"
  },
  "paths": {
    "/read-csv": {
      "get": {
        "parameters": [
          {
            "name": "file_path",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "projection_attributes",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "filter_expression",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "text/csv": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          },
          "404": { "description": "File not found" }
        }
      }
    }
  }
}
