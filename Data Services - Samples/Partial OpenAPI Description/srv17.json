{
  "openapi": "3.1.0",
  "info": {
    "title": "srv17",
    "version": "1.0.0",
	"x-service-type": "DISPATCH"
  },
  "paths": {
    "/interpolate": {
      "post": {
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "dataset": {
                    "type": "string",
                    "format": "binary"
                  },
                  "timestamp_column": {
                    "type": "string"
                  },
                  "reference_temporal_axis": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "date-time"
                    }
                  },
                  "method": {
                    "type": "string",
                    "enum": ["linear", "time", "index", "pad", "nearest", "zero", "slinear", "quadratic", "cubic", "spline", "barycentric", "polynomial", "krogh", "piecewise_polynomial", "pchip", "akima"],
                    "default": "linear"
                  }
                },
                "required": ["dataset", "timestamp_column", "reference_temporal_axis"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Interpolated dataset",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "interpolated_dataset": {
                      "type": "string",
                      "format": "binary"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid input",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
