{
  "openapi": "3.1.0",
  "info": {
    "title": "srv18",
    "version": "1.0.0",
	"x-service-type": "DISPATCH"	
  },
  "paths": {
    "/calculate-target": {
      "post": {
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "test_set": {
                    "type": "string",
                    "format": "binary"
                  },
                  "regression_coefficients": {
                    "type": "array",
                    "items": {
                      "type": "number"
                    }
                  },
                  "feature_columns": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "target_column": {
                    "type": "string"
                  }
                },
                "required": ["test_set", "regression_coefficients", "feature_columns", "target_column"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Calculated target values",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "target_values": {
                      "type": "array",
                      "items": {
                        "type": "number"
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid input",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
