{
  "openapi": "3.1.0",
  "info": {
    "title": "srv16",
    "version": "1.0.0",
	"x-service-type": "DISPATCH"
  },
  "paths": {
    "/merge-datasets": {
      "post": {
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "primary_dataset": {
                    "type": "string",
                    "format": "binary"
                  },
                  "secondary_datasets": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "binary"
                    }
                  },
                  "merge_on": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "how": {
                    "type": "string",
                    "enum": ["left", "right", "outer", "inner"],
                    "default": "inner"
                  }
                },
                "required": ["primary_dataset"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Merged dataset",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "merged_dataset": {
                      "type": "string",
                      "format": "binary"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid input",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
